{
  "name": "snippus",
  "displayName": "snippus",
  "description": "Tailus ui components code snippets built with tailwindcss in your your vs code",
  "version": "1.0.0",
  "publisher": "Tailus",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/html/button/emphasis/default.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/html/button/emphasis/medium.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/html/button/emphasis/low.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/html/button/corners/rounded.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/html/button/corners/unrounded.code-snippets"
      }
    ]
  }
}
